{
  "AppConfig": {
    "Adapters": {
      "WorkItemsAdapter": "fake",
      "VcsAdapter": "fake",
      "TerminalAdapter": "powershell",
      "BuildAdapter": "powershell"
    },
    "LivePolicy": {
      "PushEnabled": false,
      "DryRun": true,
      "RequireCredentialsValidation": true
    },
    "SemanticKernel": {
      "DefaultProvider": "openai",
      "DefaultModel": "gpt-4",
      "MaxTokens": 4096,
      "Temperature": 0.7,
      "Timeout": "00:05:00",
      "AgentServiceProviders": {
        "planner": {
          "Provider": "openai",
          "Model": "gpt-4",
          "ApiKey": null,
          "MaxTokens": 8192,
          "Temperature": 0.3
        },
        "executor": {
          "Provider": "openai",
          "Model": "gpt-3.5-turbo",
          "ApiKey": null,
          "MaxTokens": 4096,
          "Temperature": 0.7
        },
        "reviewer": {
          "Provider": "azure-openai",
          "Model": "gpt-4",
          "DeploymentName": "reviewer-gpt4",
          "ApiKey": null,
          "MaxTokens": 8192,
          "Temperature": 0.1
        }
      },
      "AgentProfiles": {
        "default": {
          "Name": "Default Agent",
          "Description": "General purpose development assistant",
          "Capabilities": [
            "coding",
            "testing",
            "documentation"
          ],
          "Settings": {
            "max_iterations": "10",
            "verbose_logging": "false"
          }
        },
        "reviewer": {
          "Name": "Code Reviewer",
          "Description": "Specialized in code review and quality assurance",
          "Capabilities": [
            "code-review",
            "testing",
            "security"
          ],
          "Settings": {
            "focus_areas": "security,performance,maintainability"
          }
        }
      }
    },
    "Ui": {
      "Settings": {
        "Theme": "Light",
        "FontSize": 9,
        "ShowStatusChips": true,
        "AutoScrollEvents": true,
        "ShowTimestamps": true,
        "MaxEventHistory": 1000
      }
    },
    "Workspace": {
      "BasePath": "./workspaces",
      "AutoCreateDirectories": true,
      "KnownRepos": {
        "junior-dev": {
          "Path": "./workspaces/junior-dev",
          "DefaultBranch": "master"
        }
      }
    },
    "Policy": {
      "Profiles": {
        "default": {
          "Name": "Default Policy",
          "ProtectedBranches": [
            "master",
            "main",
            "develop"
          ],
          "MaxFilesPerCommit": 50,
          "RequireTestsBeforePush": true,
          "RequireApprovalForPush": false,
          "Limits": {
            "CallsPerMinute": 60,
            "Burst": 10,
            "PerCommandCaps": {
              "RunTests": 5,
              "Push": 3
            }
          }
        },
        "strict": {
          "Name": "Strict Policy",
          "ProtectedBranches": [
            "master",
            "main",
            "develop",
            "release/*"
          ],
          "MaxFilesPerCommit": 20,
          "RequireTestsBeforePush": true,
          "RequireApprovalForPush": true,
          "Limits": {
            "CallsPerMinute": 30,
            "Burst": 5,
            "PerCommandCaps": {
              "Push": 1,
              "RunTests": 3
            }
          }
        }
      },
      "DefaultProfile": "default",
      "GlobalLimits": {
        "CallsPerMinute": 120,
        "Burst": 20
      }
    },
    "Transcript": {
      "Enabled": true,
      "MaxMessagesPerTranscript": 1000,
      "MaxTranscriptSizeBytes": 10485760,
      "MaxTranscriptAge": "30.00:00:00",
      "StorageDirectory": null,
      "TranscriptContextMessages": 10
    }
  }
}