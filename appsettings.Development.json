{
  "AppConfig": {
    "Auth": {
      "GitHub": {
        "Token": "your-github-personal-access-token", // Replace with your GitHub PAT - DO NOT COMMIT TO REPO
        "DefaultRepo": "your-org/your-repo" // Replace with your default repository
      },
      "Git": {
        "PersonalAccessToken": "your-git-pat", // Git PAT (alternative to SSH) - DO NOT COMMIT TO REPO
        "UserName": "Your Name" // Your Git username
      }
    },
    "Adapters": {
      "WorkItemsAdapter": "github",
      "VcsAdapter": "git"
    },
    "SemanticKernel": {
      "DefaultProvider": "openai",
      "DefaultModel": "gpt-4",
      "MaxTokens": 2048,
      "Temperature": 0.3,
      "Timeout": "00:02:00"
    }
  }
}